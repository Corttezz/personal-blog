{
  "routes": [
    { "route": "/", "serve": "/index.html" },
    { "route": "/aboutme", "serve": "/aboutme/index.html" },
    { "route": "/contact", "serve": "/contact/index.html" },
    { "route": "/blog", "serve": "/blog/index.html" }
  ],
  "navigationFallback": {
    "rewrite": "/index.html"
  },
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html"
    }
  },
  "rewriteRules": [
    {
      "source": "/{*all}.html",
      "destination": "/{all}"
    }
  ]
}
